@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix schema: <http://schema.org/> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix pav: <http://purl.org/pav/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix ex: <http://purl.org/caedA5/ontology#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dbp: <http://dbpedia.org/property/> .


<http://purl.org/caedA5/ontology#>
  a owl:Ontology ;
  owl:versionInfo "1.0" ;
  rdfs:comment "Ontologia para descrição do IDEB por estados brasileiros" ;
  dc:license <http://creativecommons.org/licenses/by/3.0/> .
  
  <http://creativecommons.org/licenses/by/3.0/> a dc:LicenseDocument.

ex:temIdeb
  a owl:ObjectProperty ;
  rdfs:domain ex:Estado  ;
  rdfs:range ex:Ideb .

ex:Estado
  a owl:Class ;
  rdfs:comment "Nome do Estado" ;
  rdfs:label "Estado" ;
  owl:disjointWith ex:Ideb .

ex:Ideb
  a owl:Class ;
  rdfs:comment "Ideb" ;
  rdfs:label "Ideb" .

ex:Evolucao
  a owl:DatatypeProperty ;
  rdfs:comment "Evolucao" ;
  rdfs:domain  ex:Ideb;
  rdfs:range xsd:double ;
  rdfs:label "Evolucao" .

ex:Aprendizado
  a owl:DatatypeProperty ;
  rdfs:comment "Aprendizado" ;
  rdfs:domain ex:Ideb;
  rdfs:range xsd:float ;
  rdfs:label "Aprendizado" .

ex:Fluxo
  a owl:DatatypeProperty ;
  rdfs:comment "Fluxo" ;
  rdfs:domain ex:Ideb ;
  rdfs:range xsd:float ;
  rdfs:label "Fluxo" .

ex:Região
  a owl:DatatypeProperty ;
  rdfs:comment "Região" ;
  rdfs:domain ex:Estado;
  rdfs:range xsd:string ;
  rdfs:label "Região" .

ex:Sigla
  a owl:DatatypeProperty ;
  rdfs:comment "Sigla do Estado" ;
  rdfs:domain ex:Estado, ex:Ideb;
  rdfs:range xsd:string ;
  rdfs:label "Sigla" .

ex:nota
  a owl:DatatypeProperty ;
  rdfs:comment "Nota" ;
  rdfs:domain ex:Ideb;
  rdfs:range xsd:float ;
  rdfs:label "nota" .




[]
  a owl:AllDisjointProperties ;
  owl:members (
   ex:Evolucao
   ex:Aprendizado
   ex:Estado
   ex:Fluxo
   ex:Sigla
   ex:Região
   ex:nota
 ) .
